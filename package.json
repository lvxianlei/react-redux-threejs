{
  "name": "web-bim",
  "version": "1.0.0",
  "description": "",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint src",
    "test": "make test",
    "start": "npm run dev:local webpack-dev-server --host 0.0.0.0",
    "dev:local": "cross-env NODE_ENV=development node build/webpack.dev.js",
    "dev:test": "cross-env NODE_ENV=development node build/webpack.dev.js --hot --deploy",
    "release": "npm run deploy:online",
    "deploy:test": "cross-env NODE_ENV=production node build/webpack.release.js --watch --uglify --deploy=test",
    "deploy:online": "cross-env NODE_ENV=production node build/webpack.release.js --uglify --deploy=online",
    "build": "cross-env NODE_ENV=production node build/webpack.release.js --uglify"
  },
  "keywords": [
    "webpack",
    "react",
    "es6",
    "babel",
    "redux",
    "boilerplate"
  ],
  "author": "xiaoyann",
  "license": "MIT",
