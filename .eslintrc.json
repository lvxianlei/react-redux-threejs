{
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "no-unreachable": "off",
    "no-redeclare": "warn"
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "modules": true
    }
  }
}